&vin5 {
	pinctrl-0 = <&vin5_pins>;
	pinctrl-names = "default";
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			vin5ep0: endpoint@0 {
				bus-width = <8>;
				/* #HSYNC, #VSYNC */
				vsync-active = <1>;
				hsync-active = <0>;
				remote-endpoint = <&cmos_camera_in>;
			};
		};
	};
};

&xhci0 {
	status = "okay";
};



