	pinctrl-0 = <&can1_pins>;
	pinctrl-names = "default";
	status = "okay";

	renesas,can-clock-select = <0x0>;
};

&canfd {
	pinctrl-0 = <&canfd0_pins &canfd1_pins>;
	pinctrl-names = "default";
	status = "disabled";

	renesas,can-clock-select = <0x0>;

	channel0 {
		status = "okay";
	};

	channel1 {
		status = "okay";
	};
};

